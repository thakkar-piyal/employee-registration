  	<servlet-class>com.EmployeeUpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeDeleteController</servlet-name>
  	<url-pattern>/EmployeeDeleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeEditController</servlet-name>
  	<url-pattern>/EmployeeEditController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeListController</servlet-name>
  	<url-pattern>/EmployeeListController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeRegistrationController</servlet-name>
  	<url-pattern>/EmployeeRegistrationController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeUpdateController</servlet-name>
  	<url-pattern>/EmployeeUpdateController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
