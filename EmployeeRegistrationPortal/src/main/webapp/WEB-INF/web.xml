  </servlet>
  <servlet>
  	<servlet-name>EmployeeRegistrationController</servlet-name>
  	<display-name>EmployeeRegistrationController</display-name>
  	<description></description>
  	<servlet-class>com.EmployeeRegistrationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeUpdateController</servlet-name>
  	<display-name>EmployeeUpdateController</display-name>
