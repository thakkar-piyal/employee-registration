<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmployeeDeleteController</servlet-name>
  	<display-name>EmployeeDeleteController</display-name>
  	<description></description>
  	<servlet-class>com.EmployeeDeleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeEditController</servlet-name>
  	<display-name>EmployeeEditController</display-name>
